{
  "name": "pizzapreference",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/cds-odata-v2-adapter-proxy": "^1.7.9",
    "@sap/logging": "^6.0.3",
    "@sap-cloud-sdk/core": "^1.47.1",
    "@sap/cds-dk": "^4.3.3",
    "@sap/hana-client": "^2.9.23",
    "@sap/jobs-client": "^1.6.1",
    "@sap/xsenv": "^3.1.1",
    "@sap/xssec": "^3.2.2",
    "body-parser": "^1.19.0",
    "express": "^4",
    "hdb": "^0.18.3",
    "cf-http-client": "^1.3.0",
    "cf-nodejs-logging-support": "^6.9.0",
    "compression": "^1.7.4",
    "helmet": "^4.6.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "sap-cf-mailer": "^0.0.5"
  },
  "devDependencies": {
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "hana_db": {
        "kind": "sql"
      },
      "local_db": {
        "kind": "sqlite",
        "model": "*"
      },
      "uaa": {
        "kind": "xsuaa"
      }
    },
    "hana": {
      "syntax": "hdi",
      "deploy-format": "hdbtable"
    }
  }
}
