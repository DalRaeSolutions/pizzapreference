{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.dalraesolutions.pizzapreference.pizzapreference",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "dataSources": {
			"pizzapreference": {
				"uri": "v2/pizza/",
				"type": "OData",
				"settings": {
                    "odataVersion": "2.0"
				}
			}
		},
        "ach": "ach",
        "crossNavigation": {
            "inbounds": {
                "com.dalraesolutions.pizzapreference-pizzapreference-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "sopizzapreference",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },

    "sap.cloud": {
		"public": true,
		"service": "pizzapreference"
    },
    
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.dalraesolutions.pizzapreference.pizzapreference.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.dalraesolutions.pizzapreference.pizzapreference.i18n.i18n"
                }
            },
            "":{
                "dataSource": "pizzapreference",
				"preload": true,
				"settings": {
					"groupId": "$auto",
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true
				}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.dalraesolutions.pizzapreference.pizzapreference.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "list",
                    "target": [
                        "list"
                    ]
                },
                {
                    "pattern": "create",
                    "name": "create",
                    "target": [
                        "create"
                    ]
                }
            ],
            "targets": {
                "list": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "List",
                    "viewName": "List"
                },
                "create": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Create",
                    "viewName": "Create"
                }
            }
        },
        "config":{
            
        }
    }
}